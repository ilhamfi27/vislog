<header class="main-header header-wrapper">
  <a href="index2.html" class="logo" style="margin-left: 200px">
    <span class="logo-mini">
      <%= image_tag 'icon/monitor.png', style: 'height: 30px' %>
    </span>
    <span class="logo-lg">
      <%= image_tag 'icon/monitor.png', style: 'height: 35px;margin-right: 10px' %>
      VISLOG
    </span>
  </a>
  <nav class="navbar navbar-static-top">
    <div class="navbar-custom-menu">
      <ul class="nav navbar-nav">
        <li class="dropdown menu-dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <i class="fa fa-th-large fa-2x"></i>
          </a>
          <ul class="dropdown-menu row box-shadow-1" style="width: 300px">
                  <%= link_to dashboards_path do %>
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-dashboard color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Dashboard
                        </p>
                      </center>
                    </li>
                  <% end %>
                  <%= link_to ads_performances_path do %>
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-certificate color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Ads Performance
                        </p>
                      </center>
                    </li>
                  <% end %>
                  <a href="clip-library.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-film color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Clip Library
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="competitive-analysis.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-user-secret color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Competitive Analysis
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="tv-analysis.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-tv color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          TV Analysis
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="advance-search.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-search color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Advance Search
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="tutorial.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-play color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Tutorial
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="update-information.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-cloud-upload color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Update Information
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="customer-service.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-phone color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Customer Service
                        </p>
                      </center>
                    </li>
                  </a>
                  <a href="pencarian-utama.html">
                    <li class="col-xs-4">
                      <center>
                        <i class="fa fa-upload color-light-blue"></i>
                        <p class="color-light-blue" 
                        style="font-size: 8pt">
                          Upload Data
                        </p>
                      </center>
                    </li>
                  </a>
                  <% if current_user.admin? %>
                    <%= link_to administrators_path do %>
                      <li class="col-xs-4">
                        <center>
                          <i class="fa fa-gear color-light-blue"></i>
                          <p class="color-light-blue" 
                          style="font-size: 8pt">
                            Administrator
                          </p>
                        </center>
                      </li>
                    <% end %>
                  <% end %>
                </ul>
        </li>
        <li class="dropdown user user-menu">
                <%= link_to destroy_user_session_path, method: :delete do %>
                  <p style="float: left;margin: 2px 10px 0px 0px">Log Out</p>
                  <i class="fa fa-sign-out fa-2x"></i>
                <% end %>
            </li>
      </ul>
