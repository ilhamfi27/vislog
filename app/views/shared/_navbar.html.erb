<aside class="main-sidebar" style="background-color: #4A89DC !important;">
  <section class="sidebar">
    <div class="user-sidebar user-panel" style="margin-top: -40px">
      <%= image_tag current_user.avatar, class: 'img-responsive img-circle', alt: 'User Image', style: 'width: 70%;margin: 0 auto;' %>
      <h4 class="color-white text-center">
        <%= current_user.name %>
      </h4>
      <h5 class="color-white text-center">
        <%= current_user.username %>
      </h5>
      <p class="text-center color-white">
        <%= current_user.email %>
      </p>
      <center>
      <%= link_to edit_user_registration_path do %>
        <button class="btn btn-default btn-khusus">
          Update Profile
        </button>
      <% end %>
      </center>
    </div>
  </section>
</aside>