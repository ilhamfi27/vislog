$('#video-modal-wrapper').html('<%= escape_javascript(render("shared/show_video_modal_on_ads_performance")) %>');
$('.css-video-modal').modal('show');

$("#video-modal-wrapper .close-modal").on('click', function(){ 
   $(".css-video-modal").html("");
   $(".css-video-modal").modal('hide');
});

var video = videojs('js-video-player', {
  controls: true,
  autoplay: false,
  preload: 'auto'
});

video.currentTime(video.currentTime() + 5);
video.on('timeupdate', function(e) {
if (video.currentTime() == 15) {
    video.pause();
  }
});
